KAAA,CAAA,4BAAA,CAAA;;AACA,UAAA,MAAA,CAAA,KAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAJA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,SANA;AAOA,OARA,MAQA;AACA,QAAA,OAAA,CAAA,SAAA,CAAA,KAAA,OAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,iCAAA,CAAA;;AACA,UAAA,MAAA,CAAA,KAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAJA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,SANA;AAOA;AACA,KAzBA;AA2BA,IAAA,OA3BA,mBA2BA,OA3BA,EA2BA;AAAA;;AAAA,UACA,EADA,GACA,OADA,CACA,EADA;;AAEA,UAAA,OAAA,CAAA,sCAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,OAAA,CAAA,EAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,+BAAA,CAAA;;AACA,UAAA,MAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,KAAA;AACA,SAJA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,SANA;AAOA;AACA,KAtCA;AAwCA,IAAA,MAxCA,kBAwCA,OAxCA,EAwCA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,KA1CA;AA4CA,IAAA,KA5CA,mBA4CA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA;AA/CA;AAnBA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <nav>\n      <div class=\"nav-wrapper blue darken-1\">\n        <a href=\"#\" class=\"brand-logo center\">Produtos</a>\n      </div>\n    </nav>\n\n    <div class=\"container\">\n      <ul>\n        <li v-for=\"(erro, index) of erros\" :key=\"index\">\n            campo <b>{{ erro.field }}</b> - {{ erro.defaultMessage }}\n        </li>\n      </ul> \n      \n      <form @submit.prevent=\"salvar\">\n        <label>Nome</label>\n        <input type=\"text\" placeholder=\"Nome\" v-model=\"produto.nome\">\n\n        <label>Quantidade</label>\n        <input type=\"number\" placeholder=\"Quantidade\" v-model=\"produto.quantidade\">\n\n        <label>Valor</label>\n        <input type=\"number\" placeholder=\"Valor\" v-model=\"produto.valor\">\n\n        <button class=\"waves-effect waves-light btn-small salvar\">\n          Salvar  <i class=\"material-icons left\">save</i>\n        </button>\n      </form>\n\n      <table>\n        <thead>\n          <tr>\n            <th>NOME</th>\n            <th>Quantidade</th>\n            <th>VALOR</th>\n            <th>OPÇÕES</th>\n          </tr>\n        </thead>\n\n        <tbody v-for=\"produto of produtos\" :key=\"produto.id\">\n          <tr>\n            <td>{{produto.nome}}</td>\n            <td>{{produto.quantidade}}</td>\n            <td>{{produto.valor}}</td>\n            <td>\n              <button @click=\"editar(produto)\" class=\"waves-effect btn-small blue darken-1\">\n                <i class=\"material-icons\">create</i>\n              </button>\n              <button @click=\"deletar(produto)\" class=\"waves-effect btn-small red darken-1\">\n                <i class=\"material-icons\">delete_sweep</i>\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\n  import Produto from './services/produtos';\n  \n  export default {\n    data() {\n      return {\n        produto: {\n          id: '',\n          nome:'',\n          quantidade:'',\n          valor:''\n        },\n\n        produtos: [],\n        erros: [],\n      }\n    },\n\n    mounted() {\n      this.listar();\n    },\n\n    methods: {\n      listar() {\n        Produto.listar().then(resposta => {\n          this.produtos = resposta.data;\n        });\n      },\n\n      salvar() {\n        if (!this.produto.id) {\n          Produto.salvar(this.produto).then(() => {\n            alert('Produto salvo com sucesso!');\n            this.clean();\n            this.listar();\n          }).catch(err => {\n            this.erros = err.response.data.errors;\n          });\n        } else {\n          Produto.atualizar(this.produto).then(() => {\n            alert('Produto atualizado com sucesso!');\n            this.clean();\n            this.listar();\n          }).catch(err => {\n            this.erros = err.response.data.errors;\n          });\n        }\n      },\n\n      deletar(produto) {\n        const { id } = produto;\n        if(confirm('Deseja realmente excluir o produto ?')){\n          Produto.deletar(id).then(() => {\n            alert('Produto deletado com sucesso!');\n            this.listar();\n            this.clean();\n          }).catch(err => {\n            this.erros = err.response.data.errors;\n          });\n        }\n      },\n\n      editar(produto) {\n        this.produto = produto;\n      },\n\n      clean() {\n        this.produto = {};\n        this.erros = [];\n      },\n    }\n  }\n\n</script>\n\n<style>\n  .salvar{\n    float: right; \n    margin-top: 20px; \n    margin-bottom: 30px;\n  }\n</style>\n"],"sourceRoot":"src"}]}